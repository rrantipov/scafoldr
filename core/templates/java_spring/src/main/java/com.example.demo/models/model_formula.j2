package com.example.models;

{% for imp in imports %}
import {{ imp }};
{% endfor %}

@Entity
@Table(name="{{ table_name }}")
@Data
@Getter @Setter
@NoArgsConstructor
@AllArgsConstructor
@Builder
public class {{ ClassName }} extends BaseModel {
    
    {%- for field in fields %}
    {%- if field.name != 'id' %}
    {%- if field.primaryKey == 'True' %}
    @Id
    @GeneratedValue(strategy = GenerationType.IDENTITY)
    {%- endif %}
    private {{ field.type }} {{ field.name }};
    {%- endif %}
    {%- endfor %}
}
